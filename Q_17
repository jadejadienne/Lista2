def main():
    
   
    Nomes = []
   
    for receber in range(0, 3):
        nome = str(input("digite um nome: "))
        Nomes.append(nome)
       
    minha_tupla = tuple(Nomes)
    print("minha_tupla:", minha_tupla)
    
    print("minha_tupla: " ," ".join(map(str, minha_tupla)))
   
    quant_maria = minha_tupla.count('Maria')
     
    print(f" 'Maria'aparece quantas vezes? {quant_maria}. ")

if __name__ == "__main__" :
      main()
